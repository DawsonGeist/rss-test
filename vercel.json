{
    "version": 2,
    "builds": [
        {
          "src": "src/**/*",
          "use": "@vercel/static-build"
        },
        { 
            "src": "api/**/*.js", 
            "use": "@vercel/node" 
        }
    ],
    "routes": [
      {
        "src": "/api/rss-proxy",
        "dest": "/api/rss-proxy.js"
      }
    ]
  }
  